{
  "name": "discord-steam-verification",
  "version": "1.0.2",
  "description": "Verify your Discord users on first join to your server and check if they have a specific app/game on their Steam library!",
  "main": "index.js",
  "scripts": {
    "dev": "devScript",
    "build": "rimraf dist && yarn --silent && tsc && rsync --recursive src/views/* dist/views/",
    "start": "cd dist && node index"
  },
  "keywords": [
    "discord",
    "steam",
    "verification"
  ],
  "author": "eggsydev",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.20",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "consola": "^2.15.0",
    "dotenv": "^8.2.0",
    "eris": "^0.14.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.1",
    "express-session": "^1.17.1",
    "steam-login": "^0.1.7"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rsync": "^0.6.1",
    "ts-devscript": "^2.0.3"
  }
}
